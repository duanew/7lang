-------------------------------------------------------------------------------
| ?- same_author('C', 'The Sum of All Fears').
no
-------------------------------------------------------------------------------
| ?- same_author('The Hunt for the Red October', 'The Sum of All Fears').
yes
-------------------------------------------------------------------------------
| ?- same_author('The Hunt for the Red October', 'The Hobbit').
no
-------------------------------------------------------------------------------
| ?- same_author('Lord of the Rings', 'The Hobbit').
yes
-------------------------------------------------------------------------------
| ?- same_instrument('David Gilmore','Mark Knopfler').

true ?
-------------------------------------------------------------------------------
guitar_players('Mark Knopfler', 'David Gilmore').
